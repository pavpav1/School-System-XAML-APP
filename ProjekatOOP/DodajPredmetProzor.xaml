<Window x:Class="ProjekatOOP.DodajPredmetProzor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjekatOOP"
        mc:Ignorable="d"
        Title="Dodaj Predmet" Height="300" Width="200">

    <Grid>
        <StackPanel>
            <Label Content="Unesi naziv" Margin="50, 0, 50, 0"/>
            <TextBox Name="txtNaziv" Margin="50, 0, 50, 0"/> 
            <Button Content="Dodaj" Click="btnDodaj_Click" Margin="50, 50, 50, 50"/>
        </StackPanel>
    </Grid>
</Window>
